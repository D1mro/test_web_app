"version": 2,
   "builds": [
     {
       "src": "server.js",
       "use": "@vercel/node"
     },
     {
       "src": "bot/bot.js",
       "use": "@vercel/node",
       "config": { "includeFiles": ["bot/**"] }
     }
   ],
   "routes": [
     {
       "src": "/webhook",
       "dest": "/bot/bot.js",
       "methods": ["POST"]
     },
     {
       "src": "/(.*)",
       "dest": "server.js"
     }
   ]
 }
