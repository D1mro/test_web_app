{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
        "src": "/bot/webhook",
        "dest": "/bot/bot.js",
        "methods": ["POST"]
      }
    ]
  }

