{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "continue": true
    }
  ]
}
