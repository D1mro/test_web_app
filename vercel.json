{
  "version": 2,
  "builds": [
    {
      "src": "bot/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "bot/bot.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
